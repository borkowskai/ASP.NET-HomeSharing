<!DOCTYPE html>
<html lang="en">
<head>
    <title>Realestate Bootstrap Theme </title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    @Styles.Render("~/Content/css")

    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/script")



    <!-- Owl stylesheet -->
    @Styles.Render("~/Content/carousel")
    @Scripts.Render("~/bundles/carousel")
    <!-- Owl stylesheet -->
    <!-- slitslider -->
    @Styles.Render("~/Content/slitslider")
    @Scripts.Render("~/bundles/slitslider")
    <!-- slitslider -->

</head>

<body>
    <!-- header -->
    @{ Html.RenderPartial("~/Views/Shared/_Header.cshtml");}
    <!-- //header -->

    @RenderBody()

    <!-- footer -->
    @{ Html.RenderPartial("~/Views/Shared/_Footer.cshtml");}
    <!-- //footer -->

</body>
</html>
